<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">


<bean id="IQuestionService" class="com.hutb.service.impl.QuestionBankServiceImpl">
    <property name="QuestionBank" ref="QuestionBank"></property>
</bean>

    <bean id="QuestionBank" class="com.hutb.domain.QuestionBank"></bean>

<bean id="IUserService" class="com.hutb.service.impl.UserServiceImpl">
    <property name="User" ref="User"></property>
</bean>
    <bean id="User" class="com.hutb.domain.User"></bean>
</beans>